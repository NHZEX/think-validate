{
    "name": "zxin/think-validate",
    "description": "think validate",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/annotations": "^2.0",
        "symfony/polyfill-php80": "^1.25",
        "topthink/framework": "^6.0.5|^6.1|^8.0",
        "zxin/think-annotation": "^2.0",
        "zxin/utils": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "~0.15",
        "phpstan/phpstan": "^1.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Think\\Validate\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/dump-validate"
    ],
    "extra": {
        "think": {
            "services": [
            ],
            "config": {
                "validate": "config/validate.php"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false
    },
    "minimum-stability": "stable",
    "repositories": [
    ]
}
